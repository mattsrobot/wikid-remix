{
  "name": "wikid",
  "version": "1.0.0",
  "description": "The wikid app",
  "main": "desktop/index.js",
  "type": "commonjs",
  "sideEffects": false,
  "scripts": {
    "web-dev": "remix dev --manual -c \"node --watch-path server.js --watch server.js\"",
    "web-start": "cross-env NODE_ENV=production node ./server.mjs",
    "web-build": "npx remix build",
    "desktop-dev": "remix dev --command \"nodemon .\" --manual",
    "desktop-build": "remix build && electron-builder",
    "desktop-start": "electron ."
  },
  "build": {
    "appId": "app.wikid.desktop",
    "productName": "Wikid",
    "copyright": "Copyright Â© 2023 Wikid",
    "files": [
      "build",
      "desktop",
      "public",
      "remix.config.js"
    ],
    "mas": {
      "category": "public.app-category.social-networking"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@radix-ui/react-accordion": "1.1.2",
